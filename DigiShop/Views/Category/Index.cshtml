@model List<Category>

<h1>Category Section</h1>
<table class = "table table-bordered table table-striped">
	<thead>
	<tr>
		<th>
			Category Name
		</th>
		<th>
			Display Order
		</th>
	</tr>
	</thead>
	<tbody>
		@foreach (var obj in Model)
		{
			<tr>
				<td>@obj.Name</td>
				<td>@obj.DisplayOrder</td>
			</tr>
		}
	</tbody>
</table>